#!/bin/bash
#SBATCH --nodes=1               #numero de nodos (maximo 13)
#SBATCH --job-name=OpenMP       #nombre del job
#SBATCH --ntasks=16
#SBATCH --time=1:00:00          #tiempo de ejecucion (maximo 168 hrs)
#SBATCH --partition=general     #cola de slurm
#SBATCH --constraint=broadwell
#SBATCH --mem-per-cpu=3200

gcc -fopenmp sortRS.c -o ordenaRS -lm


echo "Ordenamiento Regular Sampling"
rm tiempos.txt
for i in $(seq 1 16)
do
  ./ordenaRS $i datos_10M.dat >> tiempos.txt
done
